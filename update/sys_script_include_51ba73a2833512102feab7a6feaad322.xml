        <description/>
        <mobile_callable>false</mobile_callable>
        <name>Fetch User's Department</name>
        <script><![CDATA[var UserDepartmentFetcher = Class.create();
UserDepartmentFetcher.prototype = {
    initialize: function() {},

    getUserDepartment: function(userId) {
        var userDept = '';
        var userGR = new GlideRecord('sys_user');
        if (userGR.get(userId)) {
            userDept = userGR.getValue('department');
        } else {
            userDept = 'User not found';
        }
        return userDept;
    },

    type: 'UserDepartmentFetcher'
};
]]></script>
