        <name>CheckIncidentEscalation</name>
        <script><![CDATA[var IncidentEscalationChecker = Class.create();
IncidentEscalationChecker.prototype = {
    initialize: function() {},

    checkEscalationStatus: function(incidentSysId) {
        var escalationStatus = '';
        var gr = new GlideRecord('incident');
        if (gr.get(incidentSysId)) {
            escalationStatus = gr.getValue('u_escalation_status');
            if (!escalationStatus) {
                escalationStatus = 'Escalation status not set';
            }
        } else {
            escalationStatus = 'Incident not found';
        }
        return escalationStatus;
    },

    type: 'IncidentEscalationChecker'
};
]]></script>
