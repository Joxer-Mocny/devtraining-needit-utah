        <name>Location Details</name>
        <script><![CDATA[var LocationDetails = Class.create();
LocationDetails.prototype = {
    initialize: function() {},

    getLocationCoordinates: function(locationSysId) {
        var locGr = new GlideRecord('cmn_location');
        if (locGr.get(locationSysId)) {
            return locGr.latitude + ',' + locGr.longitude;
        }
        return '0,0';
    },

    type: 'LocationDetails'
};
]]></script>
