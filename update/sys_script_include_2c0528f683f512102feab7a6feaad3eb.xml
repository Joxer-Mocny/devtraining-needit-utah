        <name>EmailValidator</name>
        <script><![CDATA[var EmailValidator = Class.create();
EmailValidator.prototype = {
    initialize: function() {},

    isValidEmail: function(email) {
        var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
        return emailRegex.test(email);
    },

    type: 'EmailValidator'
};
]]></script>
