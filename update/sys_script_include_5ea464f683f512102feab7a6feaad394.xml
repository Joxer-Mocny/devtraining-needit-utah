        <name>BasicCalculator</name>
        <script><![CDATA[var BasicCalculator = Class.create();
BasicCalculator.prototype = {
    initialize: function() {},

    add: function(a, b) {
        return a + b;
    },

    subtract: function(a, b) {
        return a - b;
    },

    multiply: function(a, b) {
        return a * b;
    },

    divide: function(a, b) {
        if (b != 0) {
            return a / b;
        }
        return 'Cannot divide by zero';
    },

    type: 'BasicCalculator'
};
]]></script>
