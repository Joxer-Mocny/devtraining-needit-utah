        <name>Check User Role</name>
        <script><![CDATA[var RoleChecker = Class.create();
RoleChecker.prototype = {
    initialize: function() {},

    hasUserRole: function(roleName) {
        // This method checks if the current user has the specified role
        if (gs.hasRole(roleName)) {
            return 'User has the ' + roleName + ' role';
        } else {
            return 'User does NOT have the ' + roleName + ' role';
        }
    },

    type: 'RoleChecker'
};
]]></script>
