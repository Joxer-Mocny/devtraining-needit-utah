        <name>Incident Details</name>
        <script><![CDATA[var IncidentDetails = Class.create();
IncidentDetails.prototype = {
    initialize: function() {},

    getIncidentStatus: function(incidentNumber) {
        var incGr = new GlideRecord('incident');
        incGr.addQuery('number', incidentNumber);
        incGr.query();
        if (incGr.next()) {
            return incGr.state.getDisplayValue();
        }
        return 'Not found';
    },

    type: 'IncidentDetails'
};
]]></script>
