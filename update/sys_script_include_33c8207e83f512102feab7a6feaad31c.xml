        <description/>
        <mobile_callable>false</mobile_callable>
        <name>Group Members</name>
        <script><![CDATA[var GroupMembers = Class.create();
GroupMembers.prototype = {
    initialize: function() {},

    getMembers: function(groupSysId) {
        var members = [];
        var gr = new GlideRecord('sys_user_grmember');
        gr.addQuery('group', groupSysId);
        gr.query();
        while (gr.next()) {
            members.push(gr.user.name.toString());
        }
        return members.join(', ');
    },

    type: 'GroupMembers'
};
]]></script>
