        <name>GetUserFullName</name>
        <script><![CDATA[var GetUserFullName = Class.create();
GetUserFullName.prototype = {
	initialize: function(){
	},

	getFullName: function(userSysId){
		var gr = new GlideRecord('sys-user');
		gr.get(userSysId);
		return gr.getValue('first_name') + ' ' + gr.getValue('last_name');
	},
	type: 'GetUserFullName'
}; 
]]></script>
