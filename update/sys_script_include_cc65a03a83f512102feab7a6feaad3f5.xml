        <name>ServiceAvailabilityChecker</name>
        <script><![CDATA[var ServiceAvailabilityChecker = Class.create();
ServiceAvailabilityChecker.prototype = {
    initialize: function() {},

    isServiceAvailable: function(serviceName) {
        var now = new GlideDateTime();
        var currentHour = now.getLocalHour();
        if (serviceName == 'EmailService' && currentHour >= 9 && currentHour <= 17) {
            return true;
        }
        return false;
    },

    type: 'ServiceAvailabilityChecker'
};
]]></script>
